<%= bx_title %>

<%= bx_tabs params[:tab] %>

<div class="tab-content">
  <%= error_messages_for @form %>
  <%= labelled_form_for :form, @form, :url => project_bx_table_group_common_column_defs_path(@project, @table_group) do |f| %>
    <%= render :partial => "form", :locals => { :f => f, :form => @form, :data_types => @table_group.data_types } %>
    <%= f.submit l(:button_create) %>
    <%= link_to l(:button_cancel), project_bx_table_group_path(@project, @table_group) %>
  <% end %>
</div>
